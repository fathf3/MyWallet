<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("HomeHeader")
<body>
    <div class="container-scroller">
        <!-- partial:~/corona/partials/_sidebar.html -->
        @await Component.InvokeAsync("HomeSidebar")
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
           
            @await Component.InvokeAsync("HomeNavbar")


            <div class="main-panel">
                <div class="content-wrapper">

                    @RenderBody()


                </div>

                @await Component.InvokeAsync("HomeFooter")
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    
    @await Component.InvokeAsync("HomeScript")
    @RenderSection("Scripts", required: false)
   
</body>
</html>